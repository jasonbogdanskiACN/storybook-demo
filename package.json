{
  "name": "storybook-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve demo-app",
    "build": "ng build demo-app",
    "test": "ng test demo-app",
    "storybook": "ng run demo-app:storybook",
    "build-storybook": "ng run demo-app:build-storybook",
    "start:demo": "ng serve demo-app",
    "build:demo": "ng build demo-app",
    "test:demo": "ng test demo-app",
    "dev": "npm run storybook",
    "dev:demo": "npm run start:demo",
    "playwright": "playwright-cli",
    "playwright:help": "playwright-cli --help",
    "playwright:open": "playwright-cli open"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@primeuix/themes": "^2.0.3",
    "primeicons": "^7.0.0",
    "primeng": "^21.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2101.0",
    "@angular-devkit/build-angular": "^21.1.0",
    "@angular-devkit/core": "^21.1.0",
    "@angular/build": "^21.1.4",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/platform-browser-dynamic": "^21.1.0",
    "@compodoc/compodoc": "^1.2.1",
    "@playwright/cli": "^0.1.1",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/angular": "^10.2.8",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.1.0",
    "storybook": "^10.2.8",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
