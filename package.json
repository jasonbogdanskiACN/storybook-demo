{
  "name": "storybook-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "nx serve demo-app",
    "build": "nx build demo-app",
    "test": "nx test demo-app",
    "storybook": "nx run demo-app:storybook",
    "build-storybook": "nx run demo-app:build-storybook",
    "start:demo": "nx serve demo-app",
    "build:demo": "nx build demo-app",
    "test:demo": "nx test demo-app",
    "dev": "pnpm run storybook",
    "dev:demo": "pnpm run start:demo",
    "playwright": "playwright-cli",
    "playwright:help": "playwright-cli --help",
    "playwright:open": "playwright-cli open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.html": [
      "eslint --fix"
    ]
  },
  "private": true,
  "packageManager": "pnpm@10.30.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@compodoc/compodoc",
      "@parcel/watcher",
      "esbuild",
      "less",
      "lmdb",
      "msgpackr-extract",
      "nx"
    ]
  },
  "overrides": {
    "ajv": "^8.18.0"
  },
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@primeuix/themes": "^2.0.3",
    "primeicons": "^7.0.0",
    "primeng": "^21.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2101.0",
    "@angular-devkit/build-angular": "^21.1.0",
    "@angular-devkit/core": "^21.1.0",
    "@angular-devkit/schematics": "^21.1.4",
    "@angular-eslint/eslint-plugin": "^21.2.0",
    "@angular-eslint/eslint-plugin-template": "^21.2.0",
    "@angular-eslint/template-parser": "^21.2.0",
    "@angular/build": "^21.1.4",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/platform-browser-dynamic": "^21.1.0",
    "@compodoc/compodoc": "^1.2.1",
    "@nx/angular": "22.5.1",
    "@nx/storybook": "^22.5.1",
    "@nx/web": "22.5.1",
    "@nx/workspace": "22.5.1",
    "@playwright/cli": "^0.1.1",
    "@schematics/angular": "^21.1.4",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-mcp": "^0.2.2",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/angular": "^10.2.8",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.7",
    "ng-packagr": "^21.1.0",
    "nx": "22.5.1",
    "storybook": "^10.2.8",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
